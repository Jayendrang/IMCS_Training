package AdvanceAssignment23UnhandledException;

public class UnhandledExceptions {

	public static void main(String[] args) {
	
		String name=null;
		try {
		System.out.println(name.length());
		name="John";
		System.out.println(name.length());
		}catch(NullPointerException npe) {
			npe.getMessage()
		}
	}

}
